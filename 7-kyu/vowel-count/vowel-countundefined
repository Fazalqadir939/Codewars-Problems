#include <string>
​
using namespace std;
​
int getCount(const string& inputStr){
  int value =0;
    for (int i =0;inputStr[i]!='\0';i++)
    {
        char v = inputStr[i];
        if (v=='a')
        {
            value++;
        }if (v=='e')
        {
            value++;
        }if (v=='o')
        {
            value++;
        }if (v=='i')
        {
            value++;
        }if (v=='u')
        {
            value++;
        }else{}
    }
    //cout << value;
    return value;
}